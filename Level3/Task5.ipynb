    "#task5\n",
    "def fib(n):\n",
    "    a, b = 0, 1\n",
    "    for _ in range(n):\n",
    "        yield a\n",
    "        a, b = b, a + b\n",
    "fib_sequence = list(fib(5))\n",
    "print(fib_sequence)"
   ]
