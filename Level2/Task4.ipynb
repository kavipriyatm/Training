    "#task4\n",
    "import numpy as np\n",
    "lst=np.arange(1,6)\n",
    "even_number = filter(lambda x:x%2==0,lst)\n",
    "even_list=list(even_number)\n",
    "cubed_even=map(lambda x:x**3,even_list)\n",
    "print(list(cubed_even))"
