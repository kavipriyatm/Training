   "source": [
    "import scipy.stats as stats\n",
    "\n",
    "mean = 100\n",
    "std_dev = 15\n",
    "lower = 85\n",
    "upper = 115\n",
    "\n",
    "prob = stats.norm.cdf(upper, mean, std_dev) - stats.norm.cdf(lower, mean, std_dev)\n",
    "percentage = prob * 100\n",
    "\n",
    "print(f\"Approximately {round(percentage)}% of the data lies between 85 and 115.\")"
   ]
